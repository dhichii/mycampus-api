generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Universitas {
  id Int @id @default(autoincrement())
  nama String @db.VarChar(120)
  alamat String @db.VarChar(255)
  keterangan String @db.Text
  logo_url String @db.VarChar(255)

  @@map("universitas")
}
